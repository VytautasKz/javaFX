<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="471.0" prefWidth="473.0">
         <children>
            <Label fx:id="myLabel" layoutX="3.0" layoutY="3.0" prefHeight="154.0" prefWidth="464.0" textAlignment="CENTER">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Button layoutX="190.0" layoutY="208.0" mnemonicParsing="false" onAction="#changeText" text="press" textAlignment="CENTER" wrapText="true">
               <font>
                  <Font name="System Italic" size="24.0" />
               </font>
            </Button>
            <TextField fx:id="nameInput" layoutX="158.0" layoutY="164.0" promptText="Iveskite spejima" />
         </children>
      </Pane>
   </children>
</GridPane>
